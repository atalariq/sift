# Application
NODE_ENV=development
PORT=5000

# Database (local for development)
DATABASE_URL="postgres://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx:xxxxxxxxxxxxxxxxxxxxxxxx@db.prisma.io:5432/postgres?sslmode=require&pool=true&connection_limit=10&pool_timeout=20"

# JWT
JWT_SECRET="xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

# Google Gemini API
GEMINI_API_KEY="AIxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
GEMINI_MODEL="gemini-2.5-flash-lite"

# File Upload
MAX_FILE_SIZE=10485760

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ORIGIN=http://localhost:3000

# Supabase
SUPABASE_URL="your-supabase-url"
SUPABASE_SERVICE_KEY="your-supabase-service-key"
SUPABASE_BUCKET_NAME="candidate-cvs"
